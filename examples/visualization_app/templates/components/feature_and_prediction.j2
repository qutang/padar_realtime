<button id="feature-prediction-connect" type="button" class="btn btn-danger btn-sm">Connect</button>

<div class='card-group'>
    {% for i in range(config.max_sensors) %}
        {% with chart_id=config.feature_chart_ids[i] %}
            {% include 'components/feature_time_series_chart.j2' %}
        {% endwith %}
    {% endfor %}
</div>

<div class='card-group'>
    {% for i in range(config.num_of_tasks) %}
        {% with chart_id=config.prediction_chart_ids[i] %}
            {% include 'components/prediction_ts_chart.j2' %}
        {% endwith %}
    {% endfor %}
</div>

<script>
    console.log({{ config.feature_chart_ids }})
    new ArPipelineEngine({{ config.feature_chart_ids }}, {{ config.prediction_chart_ids }}, '{{ config.url }}', {{ config.ar_port }}, {{ config.refresh_rate }}, {{ config.window_size }} );
</script>